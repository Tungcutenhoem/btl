<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<Pane fx:id="rootPane" prefHeight="595.0" prefWidth="816.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.Scrambled.ScrambledStartScene">
    <children>
      <Pane fx:id="memStartPane" prefHeight="603.0" prefWidth="816.0" />
        <AnchorPane fx:id="scrambledPane" prefHeight="595.0" prefWidth="816.0" style="-fx-background-color: linear-gradient(to  bottom,#f194f2, #D0D5F2);">
            <children>
            <ImageView fitHeight="589.0" fitWidth="816.0" pickOnBounds="true" preserveRatio="true" />
                <Label alignment="CENTER" contentDisplay="CENTER" layoutX="101.0" layoutY="54.0" prefHeight="198.0" prefWidth="613.0" text="Scrambled Game">
                    <font>
                        <javafx.scene.text.Font name="Bell MT Bold" size="78.0" />
                    </font>
               <textFill>
                  <LinearGradient endX="1.0" endY="1.0">
                     <stops>
                        <Stop>
                           <color>
                              <Color red="0.9789473414421082" green="0.9789473414421082" blue="0.9789473414421082" />
                           </color>
                        </Stop>
                        <Stop offset="1.0">
                           <color>
                              <Color red="1.0" green="1.0" blue="1.0" />
                           </color>
                        </Stop>
                     </stops>
                  </LinearGradient>
               </textFill>
               <effect>
                  <DropShadow>
                     <color>
                        <Color red="0.9372839331626892" green="0.05925925821065903" blue="1.0" />
                     </color>
                  </DropShadow>
               </effect>
                </Label>
                <Button layoutX="575.0" layoutY="398.0" mnemonicParsing="false" onAction="#nextToGame" prefHeight="50.0" prefWidth="140.0" style="-fx-background-color: white; -fx-background-radius: 10px;" text="Let's Start">
               <font>
                  <Font name="Berlin Sans FB" size="20.0" />
               </font></Button>
                <Button layoutX="150.0" layoutY="399.0" mnemonicParsing="false" onAction="#backToStartScene" prefHeight="49.0" prefWidth="140.0" style="-fx-background-color: white; -fx-background-radius: 10px;" text="Back">
               <font>
                  <Font name="Berlin Sans FB" size="20.0" />
               </font></Button>
            </children>
        </AnchorPane>
    </children>
</Pane>
